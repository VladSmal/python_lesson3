password = input("Введдіть пароль: ")
password1 = ("Vladislav")
password2 = ("12345")
password3 = ("hillel")
if password == password1 :
    print("Ласкаво просимо")
elif password == password2 :
    print("Ласкаво просимо")
elif password == password3 :
    print("Ласкаво просимо")
else:
    print("Доступ заборонений")
